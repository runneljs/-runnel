{
  "name": "@runnel/reporter",
  "version": "0.0.0",
  "type": "module",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "node build.mjs",
    "postbuild": "tsc --project tsconfig.type.json --outdir ./dist && npm link",
    "test": "vitest run",
    "posttest": "reporter",
    "publish": "npm run build && npm publish --access public"
  },
  "devDependencies": {
    "rimraf": "^5.0.7",
    "typescript": "^5.3.3",
    "vite": "^5.3.1",
    "vitest": "^1.6.0"
  },
  "bin": {
    "runnel-report": "dist/esm/report-aggregator.js"
  },
  "files": [
    "dist/"
  ],
  "keywords": [
    "runnel",
    "bus",
    "eventbus",
    "event bus",
    "eventemitter",
    "event emitter",
    "message bus",
    "pubsub",
    "channel",
    "communication",
    "contract",
    "microfrontends",
    "microfrontend",
    "micro frontends",
    "micro frontend",
    "microservices"
  ],
  "license": "MIT"
}
